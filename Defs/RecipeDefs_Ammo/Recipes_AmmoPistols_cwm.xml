<RecipeDefs>
  
  <!-- ==================== 5.7x28mm ========================== -->

  <RecipeDef ParentName="AmmoRecipeBase">
    <defName>MakeAmmo_57x28mm</defName>
    <label>make 5.7x28mm cartridge x500</label>
    <description>Craft 500 5.7x28mm cartridges.</description>
    <jobString>Making 5.7x28mm cartridges.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>12</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ammo_57x28mm>500</Ammo_57x28mm>
    </products>
  </RecipeDef>
  
